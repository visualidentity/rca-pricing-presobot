				<div class="subscription-options-fy25--refresh" data-header-refresh></div>

				<div class="subslide-container">
					<div id="subslide-1" class="page01 subslide content active">
						<div class="arrow arrow-down" data-arrow-down></div>
						<div class="subscription-options-fy25-table-1">
							{{ table_performance.data(tableData1, 'subscription_options_table_1', 'table-subscription-option-1') }}
						</div>
					</div>

					<div id="subslide-2" class="page02 subslide content">
						<div class="arrow arrow-up" data-arrow-up></div>
